   "source": [
    "# instance 和 type 区别\r\n",
    "class A:\r\n",
    "    pass\r\n",
    "class B(A):\r\n",
    "    pass\r\n",
    "\r\n",
    "b = B()\r\n",
    "isinstance(b, B) # True\r\n",
    "isinstance(b, A) # True\r\n",
    "type(b) is B # is 判断是不是一个对象  True\r\n",
    "type(b) == B  # == 值是否相等 True\r\n",
    "type(b) is A # false"
   ],
